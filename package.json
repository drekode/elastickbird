{
  "name": "elastickbird",
  "version": "0.0.3",
  "description": "elasticsearch object modeling for nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dresiko/elastickbird.git"
  },
  "author": "Dresiko",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest ./jest/**/*.test.ts",
    "test:watch": "jest ./jest/**/*.test.ts --watch",
    "test:client": "jest ./jest/client/*.test.ts",
    "test:model": "jest ./jest/model/*.test.ts",
    "test:coverage": "jest ./jest/**/*.test.ts --coverage",
    "docker:up": "docker compose -f ./jest/docker-compose.yml up -d --quiet-pull",
    "docker:down": "docker compose -f ./jest/docker-compose.yml down",
    "docker:logs": "docker compose -f ./jest/docker-compose.yml logs -f",
    "example": "npx ts-node --transpile-only examples/basic-usage.ts",
    "example:compiled": "npm run build && node dist/examples/basic-usage.js",
    "dev": "npx ts-node --transpile-only",
    "rebuild": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint lib/**/*.ts"
  },
  "devDependencies": {
    "@clever/prettier-config": "^1.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "dependencies": {
    "@elastic/elasticsearch": "8.5.0"
  }
}
